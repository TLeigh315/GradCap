      matrix.fillRect(x4-6, 15, 12, 6, matrix.Color888(249, 120, 228));
      
      
    }
    x -= 3;
  }
}

void MsPacMan(int location, int mouth){
  
  switch(mouth){
    case 1:
      matrix.fillScreen(0);
      // fill a Yellow circle
      matrix.fillCircle(location, 16, 7, matrix.Color333(7, 7, 0));
      matrix.fillRect(location-9, 11, 4, 4, matrix.Color333(7, 0, 0));
      matrix.fillRect(location-5, 7, 4, 4, matrix.Color333(7, 0, 0));
      // draw an 'X' in black
      matrix.drawLine(location, 16, location+6, 12, matrix.Color333(0, 0, 0));
      matrix.drawLine(location, 16, location+6, 20, matrix.Color333(0, 0, 0));
      matrix.drawLine(location, 16, location+7, 16, matrix.Color333(0, 0, 0));
      matrix.fillRect(location + 3, 15, 5, 3, matrix.Color333(0, 0, 0));
      matrix.fillRect(location + 4, 13, 4, 7, matrix.Color333(0, 0, 0));
      delay(50);
      matrix.fillCircle(location, 16, 7, matrix.Color333(7, 7, 0));
      matrix.fillRect(location-9, 11, 4, 4, matrix.Color333(7, 0, 0));
      matrix.fillRect(location-5, 7, 4, 4, matrix.Color333(7, 0, 0));
      break;

    case 2:
      //matrix.fillScreen(0);
      // fill a Yellow circle
      matrix.fillCircle(location, 16, 7, matrix.Color333(7, 7, 0));
      matrix.fillRect(location+5, 11, 4, 4, matrix.Color333(7, 0, 0));
      matrix.fillRect(location+1, 7, 4, 4, matrix.Color333(7, 0, 0));
      // draw an 'X' in black
      matrix.drawLine(location, 16, location-6, 12, matrix.Color333(0, 0, 0));
      matrix.drawLine(location, 16, location-6, 20, matrix.Color333(0, 0, 0));
      matrix.drawLine(location, 16, location-7, 16, matrix.Color333(0, 0, 0));
      matrix.fillRect(location - 5, 15, 5, 3, matrix.Color333(0, 0, 0));
      matrix.fillRect(location - 7, 13, 4, 7, matrix.Color333(0, 0, 0)); 
      //delay(50);
      matrix.fillCircle(location, 16, 7, matrix.Color333(7, 7, 0));
      matrix.fillRect(location+5, 11, 4, 4, matrix.Color333(7, 0, 0));
      matrix.fillRect(location+1, 7, 4, 4, matrix.Color333(7, 0, 0));
      break;     
  }
}

